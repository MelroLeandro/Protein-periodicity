single_pdb=1;

if single_pdb==1
     pdb_ids='1UAO';
else
    % Select protein list
    pdb_file_list='pdb1A.tsv';

    % Read protein list
    pdblistID = fopen(pdb_file_list,'r');
    pdb_ids = fscanf(pdblistID, '%s');
    fprintf('File reading----\n');
    
    fclose(pdblistID);
end

%%

logfileID = fopen('proteins.txt','w');

for i=1:4:length(pdb_ids)
    
    id = pdb_ids(i:i+3);
   
    PDBstruct = getpdb(id);
    
    fprintf(logfileID,[id '-' PDBstruct.Compound(2,:) '\n']);
    
end

fclose('all');
